<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="co.edu.uniquindio.poo.proyectosameday.viewController.ShapesDealerViewController"
      spacing="20"
      styleClass="form-container">

    <Label text="Envíos asignados" styleClass="section-title"/>

    <TableView fx:id="tablaEnvios"
               styleClass="table-view">

        <columns>
            <TableColumn text="Código" fx:id="colCodigo" prefWidth="120"/>
            <TableColumn text="Destino" fx:id="colDestino" prefWidth="200"/>
            <TableColumn text="Estado" fx:id="colEstado" prefWidth="140"/>
            <TableColumn text="Fecha" fx:id="colFecha" prefWidth="160"/>
        </columns>

    </TableView>

</VBox>
